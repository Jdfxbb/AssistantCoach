<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.joshua.assistantcoach">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CoachHomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CoachLoginActivity"
            android:label="@string/title_activity_coach_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SetLineupActivity"
            android:label="@string/title_activity_coach_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RegisterActivity"
            android:label="@string/title_activity_register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RosterActivity"
            android:label="@string/title_activity_roster"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddPlayerActivity"
            android:label="@string/title_activity_add_player"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>